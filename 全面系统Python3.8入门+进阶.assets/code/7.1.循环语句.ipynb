   "source": [
    "i = 1\n",
    "while i < 6:\n",
    "    print(i, end=' ')\n",
    "    i += 1\n",
    "else:\n",
    "    print('EOF')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
