      "a b c 1 2 3 done\n"
     ]
    }
   ],
   "source": [
    "a = [['a', 'b', 'c'], (1, 2, 3)]\n",
    "for x in a:\n",
    "    for y in x:\n",
    "        print(y, end=' ')\n",
    "else:\n",
    "    print('done')"
   ]
